<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="201.0" prefWidth="372.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.ModalWindow">
   <children>
      <Text layoutX="44.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Do you really want to delete task?">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Button fx:id="noButton" layoutX="63.0" onAction="#close" layoutY="136.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="46.0" style="-fx-background-color: red; -fx-border-color: black; -fx-border-radius: 30px; -fx-background-radius: 30px;" text="No" />
      <Button fx:id="yesButton" layoutX="233.0" onAction="#delete" layoutY="136.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="46.0" style="-fx-background-color: green; -fx-border-radius: 20px; -fx-background-radius: 20px; -fx-border-color: black;" text="Yes" />
   </children>
</AnchorPane>
